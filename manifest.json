{
  "name": "s t r i p p e r",
  "short_name": "strip yo links",
  "description": "This extension was strips any page of its links.",
  "version": "0.0.1",
  "manifest_version": 2,
  "default_locale": "en",
  "background": {
    "scripts": ["src/bg/background.js"]
  },
  "browser_action": {
    "default_icon": "design/stripper-logo-square-icon.png",
    "default_title": "Strip me"
  },

  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}